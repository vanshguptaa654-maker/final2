{
  "version": 2,
  "builds": [
    {
      "src": "serverside.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
   
    {
      "source": "/customer/order/:userId/:storeName",
      "destination": "/index.html"
    },
    
   
    {
      "source": "/backend/(.*)",
      "destination": "/serverside.js" 
    },
    
  
    {
      "source": "/(.*)",
      "destination": "/index.html" 
    }
  ]
}
